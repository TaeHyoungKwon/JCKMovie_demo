    each seat in reserved
      script.
        $(".tooltip").each(function () {
            console.log("#{seat}");
            if("#{seat}" == $(this).attr("data-tooltip"))
            {
              $(this).addClass('row__seat--reserved').removeClass('tooltip').removeClass;
            }

        });


