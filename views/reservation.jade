doctype html
html.no-js(lang='en')
  head
    meta(charset='UTF-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title Perspective Seat Preview | Codrops
    meta(name='description', content='An experimental demo where a 3D perspective preview is shown for a selected seat in a cinema room.')
    meta(name='keywords', content='cinema, seat booking, seating plan, perspective, 3d')
    meta(name='author', content='Codrops')
    link(rel='shortcut icon', href='../../favicon.ico')
    link(rel='stylesheet', type='text/css', href='../../css/normalize.css')
    link(rel='stylesheet', type='text/css', href='../../css/demo.css')
    link(rel='stylesheet', type='text/css', href='../../css/component.css')
    script(src='../../js/modernizr-custom.js')
  body
    header.header
      .codrops-links
        a.codrops-icon.codrops-icon--prev(href='http://tympanus.net/Development/LineMenuStyles/', title='Previous Demo')
          span #{movie_title}
        a.codrops-icon.codrops-icon--drop(href='http://tympanus.net/codrops/?p=25885', title='Back to the article')
          span Back to the Codrops article
      h1.header__title Cinema Seat Preview
      p.note.note--screen Please view on a larger screen
      p.note.note--support Sorry, but your browser doesn't support preserve-3d!
    .container
      .cube
        .cube__side.cube__side--front
        .cube__side.cube__side--back
          .screen
            .video
            
              if movie_title == '캐리비안의 해적'
                iframe(width='1280', height='700', src='https://www.youtube.com/embed/57GUPDM1KUk', frameborder='0', allowfullscreen='')
              
              else if movie_title =='불한당'
                <iframe width="1280" height="700" src="https://www.youtube.com/embed/hLCO7_w72ko" frameborder="0" allowfullscreen></iframe>
              
              else if movie_title =='겟 아웃'
                iframe(width='1280', height='700', src='https://www.youtube.com/embed/Qu5JDOpMuEk', frameborder='0', allowfullscreen='')

              else if movie_title =='노무현입니다'
                iframe(width='854', height='480', src='https://www.youtube.com/embed/r2fomEGfapc', frameborder='0', allowfullscreen='')

            .intro.intro--shown
              .intro__side(style='float:right;')
                h2.intro__title
              .intro__side
                button.action.action--play.action--shown(style='margin-top:6px;font-size:20px;', aria-label='Play Video')
                button.action.action--seats Select your seats
        .cube__side.cube__side--left
