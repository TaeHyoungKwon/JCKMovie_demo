  body.homepage
    header#header
      .top-bar
        .container
          .row
            .col-sm-6.col-xs-4
              .top-number 
                p
                  i.fa.fa-phone-square
                  |   +82 10 5645 5999
            .col-sm-6.col-xs-8
              .social
                ul.social-share
                  li(style='margin-right:10px')
                    a(href='/login')
                      i.fa.fa-sign-in.fa-2x
                  li(style='margin-right:10px')
                    a(href='/join')
                      i.fa.fa-dribbble.fa-2x
                  li(style='margin-right:10px')
                    a(href='/login/logout')
                      i.fa.fa-sign-out.fa-2x
                  li(style='margin-right:10px')
                    if(session.user != undefined)
                      if(session.user.user_id == 'admin')
                        a(href='/admin_movie')
                          i.fa.fa-cog.fa-2x
                      else
                       span(style='color : white') #{session.user.user_name} 님 환영합니다.
                  
                    //- if(session.user != undefined)
                    //-   span #{session.user.user_name} 님 환영합니다.
